e=\"container-xxl bd-gutter flex-wrap flex-lg-nowrap\" aria-label=\"Main navigation\">\n                    <div className=\"d-lg-none\"></div>\n                    <Link to=\"/\" className=\"navbar-brand p-0 me-0 me-lg-2\" aria-label=\"\">\n                        <img className=\"logo\" src={logo} alt=\"BricksRepo logo\" />\n                    </Link>\n                    <button className=\"navbar-toggler d-flex d-lg-none order-3 p-2\" type=\"button\" data-bs-toggle=\"offcanvas\" data-bs-target=\"#bdNavbar\" aria-controls=\"bdNavbar\" aria-label=\"Toggle navigation\">\n                        <i className=\"bi bi-three-dots\"></i>\n                    </button>\n                    <div className=\"offcanvas-lg offcanvas-end flex-grow-1\" tabindex=\"-1\" id=\"bdNavbar\" aria-labelledby=\"bdNavbarOffcanvasLabel\" data-bs-scroll=\"true\">\n                        <div className=\"offcanvas-header px-4 pb-0\">\n                            <h5 className=\"offcanvas-title\" id=\"bdNavbarOffcanvasLabel\">Menu</h5>\n                            <button type=\"button\" className=\"btn-close\" data-bs-dismiss=\"offcanvas\" aria-label=\"Close\" data-bs-target=\"#bdNavbar\"></button>\n                        </div>\n                        <div className=\"offcanvas-body p-4 pt-0 p-lg-0\">\n                            <ul className=\"navbar-nav flex ms-md-auto\">\n                                <li className=\"sidebar-brand\"></li>\n                                <li className=\"nav-item py-2 px-3\">\n                                    <Link to=\"/\" className=\"nav-link\">Home</Link>\n                                </li>\n                                {\n                                    Auth.loggedIn() === false ?\n                                        <>\n                                            <li className=\"nav-item py-2 px-3\">\n                                                <Link to=\"/LoginForm\" className=\"nav-link\">Login</Link>\n                                            </li>\n                                            <li className=\"nav-item py-2 px-3\">\n                                                <Link to=\"/SignupForm\" className=\"nav-link\">Signup</Link>\n                                            </li></>\n                                        :\n                                        <li className=\"nav-item py-2 px-3\">\n                                            <a onClick={Auth.logout} className=\"nav-link\">Logout</a>\n                                        </li>\n                                }\n                                <hr>\n                                </hr>\n                            </ul>\n                        </div>\n                    </div>\n                </nav>\n            </header>\n        </>\n    );\n}\nexport default Header;","/Users/jamesmason/Desktop/Project3/client/src/pages/LoginForm.js",["54"],"import React, { useState } from 'react';\nimport { Form, Button, Alert } from 'react-bootstrap';\nimport { Link } from 'react-router-dom';\nimport { useMutation } from '@apollo/client';\n\nimport { LOGIN_USER } from '../utils/mutations';\n\nimport Auth from '../utils/auth';\n\nconst LoginForm = () => {\n\n    const [userFormData, setUserFormData] = useState({ email: '', password: '' });\n    const [validated] = useState(false);\n    const [showAlert, setShowAlert] = useState(false);\n\n    const [login, { error }] = useMutation(LOGIN_USER);\n\n    // useEffect(() => {\n    //     if (error) {\n    //         setShowAlert(true)\n    //     } else {\n    //         setShowAlert(false)\n    //     }\n    // });\n\n    const handleInputChange = (event) => {\n        const { name, value } = event.target;\n        setUserFormData({ ...userFormData, [name]: value });\n    };\n\n    const handleFormSubmit = async (event) => {\n        event.preventDefault();\n        // check if form has everything (as per react-bootstrap docs)\n        const form = event.currentTarget;\n        if (form.checkValidity() === false) {\n            event.preventDefault();\n            event.stopPropagation();\n        }\n        try {\n            const { data } = await login({\n                variables: { ...userFormData }\n            })\n            await Auth.login(data.login.token)\n            // window.location.href=\"/\"\n        } catch (error) {\n            console.error(error);\n        }\n        setUserFormData({\n            email: '',\n            password: '',\n        });\n    };\n\n    return (\n        <>\n            <div className=\"container login mb-153\">\n                <div className='row py-5 form-bg'>\n                    <h1 className='mb-3'>Login to BricksRepo</h1>\n                    <p>Don't have a user account? <Link to=\"/SignupForm\" className='blue'>Sign up</Link> for one now.</p>\n                    <div className='col-7'>\n                        <Form noValidate validated={validated} onSubmit={handleFormSubmit}>\n                            <Alert dismissible onClose={() => setShowAlert(false)} show={showAlert} variant='danger'>\n                                Something went wrong with your login credentials!\n                            </Alert>\n                            <Form.Group>\n                                <Form.Label htmlFor='email'>Email</Form.Label>\n                                <Form.Control\n                                    type='text'\n                                    placeholder='Your email'\n                                    name='email'\n                                    onChange={handleInputChange}\n                                    value={userFormData.email}\n                                    required\n                                />\n                                <Form.Control.Feedback type='invalid'>Email is required!</Form.Control.Feedback>\n                            </Form.Group>\n\n                            <Form.Group>\n                                <Form.Label htmlFor='password'>Password</Form.Label>\n                                <Form.Control\n                                    type='password'\n                                    placeholder='Your password'\n                                    name='password'\n                                    onChange={handleInputChange}\n                                    value={userFormData.password}\n                                    required\n                                />\n                                <Form.Control.Feedback type='invalid'>Password is required!</Form.Control.Feedback>\n                            </Form.Group>\n                            <Button\n                                disabled={!(userFormData.email && userFormData.password)}\n                                type='submit'\n                                variant='success'>\n                                Submit\n                            </Button>\n                        </Form>\n                    </div>\n                </div>\n            </div>\n        </>\n    );\n}\n\nexport default LoginForm;","/Users/jamesmason/Desktop/Project3/client/src/pages/Home.js",[],"/Users/jamesmason/Desktop/Project3/client/src/components/SetSearchForm.js",["55","56","57"],"import React, {useState} from 'react';\nimport SetSearchResults from './SetSearchResults';\nimport { useLazyQuery } from '@apollo/client';\nimport { GET_SET } from '../utils/queries';\n\nconst SetSearchForm = () => {\n  const [searchInput, setSearchInput]= useState(\"\")\n  const [ getSet, {loading, data, error} ] = useLazyQuery(GET_SET)\n\n\n\n   const handleFormSubmit = async (event) => {\n       event.preventDefault();\n\n      if (!searchInput) {\n          return false;\n      }\n      console.log(searchInput)  //correct\n      const set = data?.set || []\n      try {\n        await getSet ({\n          variables: {Name: searchInput}\n        });\n      } catch (err) {\n        console.error(err);\n      }\n    };\n    // console.log(set)\n    // setSearchInput(\"\")\n    \n    console.log(data)\n\n  return (\n    <>\n      <form onSubmit={handleFormSubmit}>\n        <div className=\"form-row\">\n          <div className=\"col-md-8 col-12\">\n            <input\n              name=\"searchInput\"\n              placeholder=\"Search for a set\"\n              type=\"text\"\n              className=\"form-control form-control-lg\"\n              value={searchInput}\n              onChange={(e) => setSearchInput(e.target.value)}\n            />\n          </div>\n          <div class=\"col-md-4 col-12\">\n            <button type=\"submit\" className=\"btn btn-blue mt-3\">\n              Submit Search\n            </button>\n          </div>\n        </div>\n      </form>\n      <div className='container mb-5 mb-153'>\n                <div className=\"row d-flex\">\n                    <SetSearchResults\n                        set={data?.set}\n                    />\n                </div>\n            </div> \n    </>\n  );\n};\n\nexport default SetSearchForm;\n","/Users/jamesmason/Desktop/Project3/client/src/components/SetSearchResults.js",[],"/Users/jamesmason/Desktop/Project3/client/src/utils/mutations.js",[],"/Users/jamesmason/Desktop/Project3/client/src/utils/auth.js",[],"/Users/jamesmason/Desktop/Project3/client/src/utils/queries.js",[],{"ruleId":"58","severity":1,"message":"59","line":39,"column":45,"nodeType":"60","endLine":39,"endColumn":91},{"ruleId":"61","severity":1,"message":"62","line":16,"column":21,"nodeType":"63","messageId":"64","endLine":16,"endColumn":26},{"ruleId":"61","severity":1,"message":"65","line":8,"column":20,"nodeType":"63","messageId":"64","endLine":8,"endColumn":27},{"ruleId":"61","severity":1,"message":"62","line":8,"column":35,"nodeType":"63","messageId":"64","endLine":8,"endColumn":40},{"ruleId":"61","severity":1,"message":"66","line":19,"column":13,"nodeType":"63","messageId":"64","endLine":19,"endColumn":16},"jsx-a11y/anchor-is-v